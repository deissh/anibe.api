{
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "env": {
    "jest": true,
    "jasmine": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
